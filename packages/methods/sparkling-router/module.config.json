{
  "name": "sparkling-router",
  "platforms": ["android", "ios"],
  "version": "1.0.0",
  "description": "Router methods for navigation and page management in Sparkling apps",
  "methods": {
    "open": {
      "description": "Navigate to a new page or route",
      "parameters": {
        "scheme": "string",
        "params": "Record<string, any>",
        "options": "Record<string, any>"
      },
      "returns": {
        "success": "boolean",
        "message": "string",
        "data": "any"
      }
    },
    "close": {
      "description": "Close the current page or route",
      "parameters": {
        "result": "any",
        "animated": "boolean"
      },
      "returns": {
        "success": "boolean"
      }
    }
  },
  "android": {
    "packageName": "com.tiktok.sparkling.methods.router",
    "className": "RouterMethod",
    "buildGradle": "android/build.gradle.kts"
  },
  "ios": {
    "moduleName": "SPKRouter",
    "className": "RouterMethod",
    "podspecPath": "ios/Sparkling-SPKRouter.podspec",
    "swiftVersion": "5.0"
  },
  "web": {
    "entryPoint": "index.ts",
    "bundleName": "router.bundle"
  }
}