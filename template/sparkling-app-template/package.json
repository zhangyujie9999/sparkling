{
  "name": "sparkling-app-template",
  "version": "2.0.0",
  "description": "Brownfield Lynx app based on Sparkling",
  "type": "module",
  "scripts": {
    "build": "sparkling-cli build",
    "build:copy": "sparkling-cli build --copy",
    "autolink": "sparkling-cli autolink",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "android": "sparkling-cli run:android",
    "ios": "sparkling-cli run:ios",
    "run:android": "sparkling-cli run:android",
    "run:ios": "sparkling-cli run:ios",
    "android:copy": "sparkling-cli run:android --copy",
    "ios:copy": "sparkling-cli run:ios --copy",
    "run:android:copy": "sparkling-cli run:android --copy",
    "run:ios:copy": "sparkling-cli run:ios --copy"
  },
  "dependencies": {
    "@lynx-js/react": "^0.107.0",
    "sparkling-router": "rc"
  },
  "devDependencies": {
    "@lynx-js/qrcode-rsbuild-plugin": "^0.3.6",
    "@lynx-js/react-rsbuild-plugin": "^0.9.8",
    "@lynx-js/rspeedy": "^0.9.3",
    "@lynx-js/types": "^3.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^18.3.20",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vitest": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "sparkling-cli": "rc"
  },
  "engines": {
    "node": ">=18"
  },
  "private": false
}
